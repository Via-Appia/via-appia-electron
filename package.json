{
  "name": "via-appia-electron",
  "version": "1.0.0",
  "platform": "win x64",
  "main": "build/electron.main.js",
  "license": "MIT",
  "author": "Jorne Envy <jornejongsma@gmail.com>",
  "company": "ProductBuilder",
  "productName": "Via Appia Museum App",
  "description": "Via Appia Museum App for Het Valkhof Nijmegen",
  "scripts": {
    "start:web": "webpack serve --config-name webConfig",
    "start:render": "webpack --config-name devRenderConfig",
    "start:electron": "webpack --config-name devElectronConfig",
    "start:run-electron": "nodemon --exec electron build/electron.main.js APP_DEV",
    "start": "npm-run-all -l -p start:*",
    "build:web": "webpack --config-name webConfig",
    "build:render": "webpack --config-name buildRenderConfig",
    "build:electron": "webpack --config-name buildElectronConfig",
    "build": "npm-run-all -l -s build:*",
    "dist": "yarn build && electron-builder build --win --publish never"
  },
  "dependencies": {
    "@xstate/inspect": "^0.6.0",
    "@xstate/react": "^1.6.3",
    "d3": "^7.3.0",
    "electron-updater": "^4.3.9",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "ws": "^7.2.0",
    "xstate": "^4.27.0",
    "zustand": "^3.6.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "bufferutil": "^4.0.6",
    "css-loader": "^5.2.5",
    "electron": "^10.1.5",
    "electron-builder": "^22.10.5",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "utf-8-validate": "^5.0.8",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.7.3",
    "yarn-run-all": "^3.1.1"
  }
}
